{
    "name": "Respectful Private Redirects",
    "version": "1.0",
    "description": "Limited redirects that doesn't need permissions to all sites",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://youtu.be/*",
        "*://*.youtube.com/*",
        "*://*.youtube-nocookie.com/*",
        "*://*.instagram.com/*",
        "*://*.twitter.com/*",
        "*://*.reddit.com/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "manifest_version": 2
}
